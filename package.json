{
  "name": "screen-recorder",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/*.html dist/ && copyfiles -u 1 src/public/* dist/",
    "start": "electron-forge start",
